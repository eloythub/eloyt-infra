{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "eloyt-nginx",
    "labels": {
      "app": "webapp"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "eloyt-nginx",
        "image": "nginx:alpine",
        "ports": [
          {
            "name": "eloyt-nginx",
            "containerPort": 80
          }
        ]
      }
    ]
  }
}